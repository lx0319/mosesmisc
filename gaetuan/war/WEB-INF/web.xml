<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Gaetuan</servlet-name>
		<servlet-class>net.liuxuan.GaetuanServlet</servlet-class>
	</servlet>
	<servlet>
    	<servlet-name>tuansite</servlet-name>
    	<servlet-class>net.liuxuan.tuan.site.servlet.TuanSiteServlet</servlet-class>
  	</servlet>
  	<!--mapping-->
	<servlet-mapping>
		<servlet-name>Gaetuan</servlet-name>
		<url-pattern>/gaetuan</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
    	<servlet-name>tuansite</servlet-name>
    	<url-pattern>/tuansite</url-pattern>
  	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
